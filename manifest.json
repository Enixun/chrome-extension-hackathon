{
    "manifest_version": 2,
    "name": "Cat Pun Name",
    "version": "0.0",
    "description": "Blocks out distracting sites with a more distracting cat",
    "content_scripts": [
        {
            "matches": [
                "https://www.facebook.com/*", 
                "https://www.youtube.com/*", 
                "https://www.reddit.com/*",
                "https://www.twitter.com/*",
                "https://www.instagram.com/*"
            ],
            "js": ["./main.js"]
        }
    ]
}
{
    "manifest_version": 2,
    "name": "Centering Attention Tool",
    "version": "1.0",
    "description": "Distracting you from your distractions",
    "content_scripts": [
        {
            "matches": [
                "https://www.facebook.com/*", 
                "https://www.youtube.com/*", 
                "https://www.reddit.com/*",
                "https://www.twitter.com/*",
                "https://www.instagram.com/*"
            ],
            "js": ["./main.js"]
        }
    ]
}